{
    "model": "ContrastPool",
    "params": {
        "seed": 41,
        "epochs": 1000,
        "batch_size": 20,
        "init_lr": 1e-2,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 25,
        "min_lr": 1e-6,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 30
    },
    
    "net_params": {
        "L": 2,
        "hidden_dim": 100,
        "out_dim": 100,
        "residual": true,
        "readout": "mean",
        "in_feat_dropout": 0.0,
        "dropout": 0.0,
        "batch_norm": true,
        "sage_aggregator": "maxpool",
        "edge_feat": false,
        "lambda1": 0.0001,
        "learnable_q": false,
        "contrast": true
    }
}