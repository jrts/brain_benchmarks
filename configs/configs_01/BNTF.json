{
    "model": "BNTF",
    "params": {
        "seed": 41,
        "epochs": 1000,
        "batch_size": 20,
        "init_lr": 1.0e-4,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 25,
        "min_lr": 1e-6,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 12
    },
    
    "net_params": {
        "L": 4,
        "sizes": [256, 100],
        "pooling": [false, true],
        "pos_encoding": "identity",
        "pos_embed_dim": 128,
        "freeze_center": true,
        "orthogonal": true,
        "edge_feat": false,
        "project_assignment": true
    }
}
